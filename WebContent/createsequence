

call NEXT VALUE FOR request;

INSERT INTO request VALUES (NEXT VALUE FOR request, '1','0','2014-05-12')

CREATE SEQUENCE request AS INTEGER START WITH 101 INCREMENT BY 1;
CREATE SEQUENCE project_id AS INTEGER START WITH 1001 INCREMENT BY 1;
CREATE SEQUENCE requirement_id AS INTEGER START WITH 1001 INCREMENT BY 1;
CREATE SEQUENCE emp_id AS INTEGER START WITH 100000 INCREMENT BY 1;